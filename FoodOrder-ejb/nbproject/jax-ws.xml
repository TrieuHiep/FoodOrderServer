<?xml version="1.0" encoding="UTF-8"?>
<jax-ws xmlns="http://www.netbeans.org/ns/jax-ws/1">
  <services>
    <service name="personws">
      <implementation-class>ws.personws</implementation-class>
    </service>
    <service name="customerws">
      <implementation-class>ws.customerws</implementation-class>
    </service>
    <service name="mealws">
      <implementation-class>ws.mealws</implementation-class>
    </service>
    <service name="burgerws">
      <implementation-class>ws.burgerws</implementation-class>
    </service>
    <service name="accountws">
      <implementation-class>ws.accountws</implementation-class>
    </service>
    <service name="foodorderws">
      <implementation-class>ws.foodorderws</implementation-class>
    </service>
    <service name="foodws">
      <implementation-class>ws.foodws</implementation-class>
    </service>
    <service name="chickenws">
      <implementation-class>ws.chickenws</implementation-class>
    </service>
    <service name="ricews">
      <implementation-class>ws.ricews</implementation-class>
    </service>
  </services>
  <clients>
    <client name="customerws">
      <wsdl-url>http://localhost:8080/customerws/customerws?WSDL</wsdl-url>
      <local-wsdl-file>localhost_8080/customerws/customerws.wsdl</local-wsdl-file>
      <package-name forceReplace="true">testws</package-name>
      <catalog-file>catalog.xml</catalog-file>
      <wsimport-options>
        <wsimport-option>
          <wsimport-option-name>extension</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>verbose</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>fork</wsimport-option-name>
          <wsimport-option-value>false</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>wsdlLocation</wsimport-option-name>
          <wsimport-option-value>http://localhost:8080/customerws/customerws?WSDL</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xnocompile</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xendorsed</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>package</wsimport-option-name>
          <wsimport-option-value>testws</wsimport-option-value>
        </wsimport-option>
      </wsimport-options>
    </client>
  </clients>
</jax-ws>
